.PHONY: clean

FLAGS           = -w A -w -6 -warn-error A
OCAMLFLAGS      += $(FLAGS)
OCAMLCFLAGS     += $(FLAGS)
OCAMLOPTFLAGS   += $(FLAGS)

Clean() =
	rm -f ./*.o ./*.cmo ./*.cmx ./*.cmi ./*.cmt ./*.annot ./*.cgi ./*.opt
	rm -f ./*~
	rm -f ./OMakefile.omc

.SUBDIRS: lib api script

clean:
	Clean()
